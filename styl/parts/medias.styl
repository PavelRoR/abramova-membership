@media screen and (min-width 2299px)
    .container
        width 1400px
    .online
        text-align: center
    .main_pre
        margin-left: 264px
    .main_title
        margin-left: 230px
    .second_text
        font-size: 20px
        max-width 1000px
    .for_row
        max-width 900px
    .for_title
        margin-bottom: 59px
    .for_container
    .price_container
        max-width 428px
    .for_container_text
    .how_description_text
    .how_attention_block p
        font-size: 20px
    .reach_title
    .reach_list
        margin-left: 240px
    .how_attention_block
        height 130px
    .prices_aftertitle
        font-size: 22px
        max-width 1000px
    #footer #conditions, #footer p
        font-size: 18px
    .soc_buttons a .fa
        width 45px
        height 45px
        line-height:  45px
        font-size: 28px
    .how_description
        padding 50px 70px 43px
@media screen and (max-width 1199px)
    #logo
        width: 200px
        margin-top: 17px
    .main_pre
        margin-left: 60px
    .main_title
        margin-left: 30px
    #janna_main
        right -180px
    .reach_title
    .reach_list
        margin-left: 20px
@media screen and (max-width 991px)
    #logo
        width: 180px
        margin-top: 8px
    .online
        font-size: 16px
        margin-top: 8px
    .contacts .fa, .contacts span
        font-size: 12px
    #main
        padding 50px 0
    .main_pre
        margin-left: 0
        font-size: 30px
        display: inline
        max-width none
        padding 5px 10px
    .main_title
        font-size: 100px
        margin 80px 0 40px 20px
    #janna_main
        width: 645px
        right -170px
    #second
    #reach
    #how
    #for
        padding 60px 0
    .how_description
        padding 50px 55px 40px
    .how_description_time
        right 10px
        span
            letter-spacing: -1px
    #reach
        background-position: center
    #footer
        padding 20px 0
@media screen and (max-width 767px)
    #header
        padding 10px 0
    #logo
        width 160px
        display block
        margin 0 auto
    .online
        text-align: center
        margin: 5px 0
    .contacts_container
        width 100%
        max-width 320px
        margin 0 auto
        .col-xs-6
            padding-left: 5px
            padding-right: 5px
    .contacts
        text-align: center
    #main
        padding 40px 0
        text-align: center
    .main_title
        margin 80px 0 40px
        span
            margin-left: 0
    #janna_main
        display: none
    #how, #reach, #second,#prices,#for
        padding 40px 0
    .for_general_title
        display block
    .for_aux_title
        display: none
    .for_container
        margin-bottom: 20px
    .reach_single
        max-width 460px
        margin-bottom: 30px
    .reach_single:before
        height 150px
        bottom: 90px
    .reach_single_1:before
        height 60px
    .prices_title
        margin-bottom: 30px
    .how_description:before
        width 100%
        max-width 60%
@media screen and (max-width 650px)
    .reach_single:after
        width: 50px
    .reach_single_text
        padding-left: 60px
    .how_description:before
        width 100%
        max-width 55%
@media screen and (max-width 560px)
    .how_description:before
        max-width 50%
@media screen and (max-width 485px)
    .online
        font-size: 14px
    .contacts_container
        max-width 295px
    .contacts .fa, .contacts span
        font-size: 11px
    #main
    #how, #prices, #reach, #second
    #for
        padding 30px 0
    .main_pre
        font-size: 24px
    .main_title
        font-size: 70px
        margin 50px 0 30px
    .main_bitton
        font-size: 24px
        max-width 260px
    .redline_button
        height 45px
        line-height: 41px
        font-size: 24px
        max-width 260px
    .second_text
    .red_divider
        margin-bottom: 15px
    .for_title
        font-size: 24px
    .for_container
        padding 15px 20px
    .for_container_img
        width 120px
        height 120px
    .for_container_img_2
        background-position: -240px 0
    .for_container_img_3
        background-position: -480px 0
    .for_container_img_4
        background-position: -120px 0
    .for_container_img_5
        background-position: -360px 0
    .for_container_img_6
        background-position: -600px 0
    .reach_title
        font-size: 24px
        padding-left: 20px
    .reach_list
        padding-top: 30px
    .reach_list, .reach_title
        margin-left: 10px
    .reach_single
        margin-bottom: 25px
    .reach_single_text
        font-size: 18px
        padding-left: 48px
    .reach_single:after
        width: 40px
    .reach_single:before
        bottom: 45px
    .how_title
        font-size: 24px
        margin-bottom: 30px
    .how_subtitle
        font-size: 24px
        height 45px
        line-height: 37px
        max-width 260px
    .how_description
        padding 30px
        margin 40px auto 55px
    .how_description_time
        font-size: 34px
    .how_attention
        margin-bottom: 35px
    .how_attention_title
        font-size: 36px
        top -18px
    .how_attention::after, .how_attention::before
        width: 18%
    .how_attention_block
        padding 10px 10px 10px 100px
        background-position: 16px center
    .how_attention_block_3
        background-position: 21px center
    .prices_title
        font-size: 26px
    .prices_aftertitle
    #footer h5
        font-size: 18px
    .soc_buttons a
        margin 0 8px 15px
    #footer #conditions, #footer p
        font-size: 12px